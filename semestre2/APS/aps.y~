%{

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

%}

%token  NOMBRE
%token  ADD  SUB MUL  DIV
%token  fdl

%%

prog:
sequence// {$$ = 'prog('$1')' ;};

sequence:
expression
|expression sequence;

expression:
NOMBRE {$$ = $1;}
| '('ADD' 'expression' 'expression')'  {printf("app(add,%d,%d)",$,2);}

%%
int yyerror(void){
  fprintf(stderr, "erreur de syntaxe\n");
  return 1;
}

int main(int argc, char** argv) {
    yyparse();
    return 0;
}
